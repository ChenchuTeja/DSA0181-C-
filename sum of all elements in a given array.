#include <iostream>
using namespace std;
int main(){
	int n;
	cout<<"enter the num of valus in array: ";
	cin>>  n;
	int a[n];
	cout<<"enter the values":
		for(int i=0;i<n;i++){
			cin>>a[i];
		}
		int sum=0;
		for(int i=0;i<n;i++){
			sum+=a[i];
		}
		cout<<"sum of digits : "<<sum<<endl;
		return 0;
	

}
