#include <iostream>
using namespace std;
int main(){
	int n;
	cout<<"enter the num of valus in array: ";
	cin>>  n;
	int arr[n];
	cout<<"enter the values":
		for(int i=0;i<n;i++){
			cin>>arr[i];
		}
		
		int max=[0],min=[0];
		for(int i=0;i<n;i++){
			if(arr[i]>max){
				max=arr[i];
			}
			if(arr[i]<min){
				min=arr[i];
			}
		}
		cout<<"max value: "<<max<<endl;
		cout<<"min value: "<<min<<endl;
		return 0;
}

